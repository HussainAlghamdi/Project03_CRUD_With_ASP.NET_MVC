@model StudentModel
@{ 
 var Students = (StudentModel)ViewData["Students"];
}
<form asp-action="Edit" method="post" class="from-horizontal">

    <div class="from-group">
        <label asp-for="Id">Id</label>
        <input type="hidden" asp-for="Id" value="@Students.Id" disabled />
    </div>

    <div class="from-group">
        <label asp-for="Name">Name</label>
        <input asp-for="Name" value="@Students.Name" />
    </div>

    <div class="from-group">
        <label asp-for="PhoneNum">PhoneNum</label>
        <input asp-for="PhoneNum" value="@Students.PhoneNum" />
    </div>

    <div class="from-group">
        <label asp-for="DateOfBirth">DateOfBirth</label>
        <input asp-for="DateOfBirth" value="@Students.DateOfBirth" />
    </div>

    <div class="from-group">
        <label asp-for="Major">Major</label>
        <input asp-for="Major" value="@Students.Major" />
    </div>

    <div class="from-group">
        <label asp-for="CollegeId">CollegeId</label>
        <input asp-for="CollegeId" value="@Students.CollegeId" />
    </div>

    <div class="from-group">
        <label asp-for="InstructorId">InstructorId</label>
        <input asp-for="InstructorId" value="@Students.InstructorId" />
    </div>


    <div class="from-group">

        <input type="submit" class="btn-btn-primary" value="Update" />
    </div>



</form>